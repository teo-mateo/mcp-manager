{
  "mcpServers": {
    "global-browser-tools": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@agentdeskai/browser-tools-mcp"],
      "env": {}
    }
  },
  "mcpServers_disabled": {
    "global-disabled": {
      "type": "stdio",
      "command": "disabled-global",
      "args": ["--global"]
    }
  },
  "projects": {
    "/github/teo-mateo/mcp-manager": {
      "mcpServers": {
        "test-server": {
          "command": "node",
          "args": ["test-server.js"],
          "env": {
            "TEST_VAR": "value"
          }
        },
        "python-server": {
          "command": "python",
          "args": ["-m", "mcp_server"],
          "env": {}
        }
      },
      "mcpServers_disabled": {
        "disabled-server": {
          "command": "disabled",
          "args": ["--test"]
        }
      }
    }
  }
}